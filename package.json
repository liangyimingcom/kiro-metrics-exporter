{
  "name": "kiro-metrics-exporter",
  "displayName": "Kiro Metrics Exporter",
  "description": "Export metrics to AWS S3",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/example/kiro-metrics-exporter.git"
  },
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Metrics Exporter",
      "properties": {
        "metricsExporter.aws.accessKey": {
          "type": "string",
          "default": "",
          "description": "AWS Access Key ID for S3 uploads",
          "scope": "application"
        },
        "metricsExporter.aws.secretKey": {
          "type": "string",
          "default": "",
          "description": "AWS Secret Access Key for S3 uploads",
          "scope": "application"
        },
        "metricsExporter.aws.region": {
          "type": "string",
          "default": "us-east-1",
          "description": "AWS region for S3 operations",
          "scope": "application"
        },
        "metricsExporter.aws.s3Prefix": {
          "type": "string",
          "default": "",
          "description": "S3 prefix path (e.g., s3://bucket/prefix/AWSLogs/accountId/KiroLogs/by_user_analytic/Region/)",
          "scope": "application"
        },
        "metricsExporter.aws.userId": {
          "type": "string",
          "default": "",
          "description": "AWS Identity Center User ID",
          "scope": "application"
        },
        "metricsExporter.aws.identityStoreId": {
          "type": "string",
          "default": "",
          "description": "AWS Identity Store ID for user lookup",
          "scope": "application"
        }
      }
    },
    "views": {
      "explorer": [
        {
          "id": "metricsExporter",
          "name": "Metrics Exporter",
          "when": "true"
        }
      ]
    },
    "commands": [
      {
        "command": "metricsExporter.setAccessKey",
        "title": "Metrics Exporter: Set Access Key"
      },
      {
        "command": "metricsExporter.setSecretKey",
        "title": "Metrics Exporter: Set Secret Key"
      },
      {
        "command": "metricsExporter.setS3Prefix",
        "title": "Metrics Exporter: Set S3 Prefix"
      },
      {
        "command": "metricsExporter.setUserId",
        "title": "Metrics Exporter: Set User ID"
      },
      {
        "command": "metricsExporter.setIdentityStoreId",
        "title": "Metrics Exporter: Set Identity Store ID"
      },
      {
        "command": "metricsExporter.uploadLastWeek",
        "title": "Upload Last 7 Days",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "metricsExporter.uploadLastMonth",
        "title": "Upload Last 30 Days",
        "icon": "$(cloud-upload)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "metricsExporter.uploadLastWeek",
          "when": "view == metricsExporter",
          "group": "navigation"
        },
        {
          "command": "metricsExporter.uploadLastMonth",
          "when": "view == metricsExporter",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/vscode": "^1.103.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-identitystore": "^3.957.0",
    "@aws-sdk/client-s3": "^3.957.0",
    "diff": "^8.0.2"
  }
}